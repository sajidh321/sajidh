import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner s= new Scanner(System.in);
        int N=s.nextInt();
        for(int i = 0; i < N; i++){
            for(int j = 0; j < N; j++){
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
