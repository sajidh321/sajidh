import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner (System.in);
        int N=s.nextInt();
        if(N>0)
        {
            System.out.print("Positive");
        }
        else{
            System.out.print("Negative");
        }
     
    }
}
