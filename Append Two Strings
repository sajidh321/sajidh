import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
     
        
        Scanner scanner = new Scanner(System.in);

        
        String S1 = scanner.nextLine();

        
        String S2 = scanner.nextLine();

        
        String result = S1 + S2;

        
        System.out.println(result);

        
        scanner.close();
    }
}
