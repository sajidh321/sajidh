import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
     Scanner s=new Scanner(System.in);
        int a=s.nextInt();
            if(a%5==0){
                if(a%1==0){
                    System.out.print("Yes");
                    
                }
                else
                {
                    System.out.print("No");
                 }
            }
            
    }
}
