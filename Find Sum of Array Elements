import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner s=new Scanner(System.in);
        int i;
        int N=s.nextInt();
        int a[]=new int[10];
        int sum=0;
       
        for(i=0;i<N;i++)
        {
            int num=s.nextInt();
            sum+=num;
        }
   
       System.out.println(sum);
    }
}
